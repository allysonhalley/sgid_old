<div class="modal fade" id='<%="modal-destroy-#{object_cancel.id}"%>' role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title">
                      <%= StrHelper.system_i18n(:sure, [:helpers, :submit])%>
                    </h4>
                </div>
                <div class="modal-body">
                    <div class="box box-danger">
                        <div class="form-horizontal">
                            <div class="box-body">
                                <p><%= object_cancel.register_and_document %></p>
                            </div>
                        </div>                    
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default btn-xs pull-left" data-dismiss="modal">
                      <%= StrHelper.system_i18n(:back, [:helpers, :layouts])%>
                    </button>
                    <%= button_to(StrHelper.system_i18n(:cancel, [:helpers, :layouts]), object_cancel, method: :delete, class: 'btn btn-danger btn-xs') %>
                </div>
            </div>
        </div>
    </div>